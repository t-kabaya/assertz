{
  "compilerOptions": {
    "outDir": "dist",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
  },
  "indlude": [
    "*/"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}