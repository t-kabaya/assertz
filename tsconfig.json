{
  "compilerOptions": {
    "outDir": "dist",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "es5",
    "lib": [
      "es2019"
    ]
  },
  "exclude": [
    "dist",
    "node_modules"
  ]
}